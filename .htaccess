# Disabling MultiViews prevents a resource being accessed without it's extension.
<IfModule mod_negotiation.c>
    Options -MultiViews
</IfModule>

<IfModule mod_rewrite.c>
    RewriteEngine On

    # Route every request through the fugue front controller.
    RewriteRule ^ fugue.php [QSA,L]
</IfModule>
